{
  "manifest_version": 3,
  "name": "dota2.ru_ext",
  "version": "3.0.3",
  "description": "dota2.ru_ext",
  "permissions": [
    "scripting",
    "storage"
  ],
  "host_permissions": [
    "https://dota2.ru/*"
  ],
  "content_scripts": [
    {
      "matches": [
        "https://dota2.ru/*"
      ],
      "js": [
        "content.js"
      ],
      "run_at": "document_start"
    }
  ],
  "web_accessible_resources": [
    {
      "resources": [
        "injected.js"
      ],
      "matches": [
        "<all_urls>"
      ]
    }
  ],
  "icons": {
    "16": "icons/16.png",
    "32": "icons/32.png",
    "48": "icons/48.png",
    "128": "icons/128.png"
  }
} 